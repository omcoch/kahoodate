  navigator.clipboard.writeText(newUuid)\r\n  }\r\n\r\n\r\n  return (\r\n    <Container className=\"align-items-center d-flex\" style={{ height: '100vh' }}>\r\n      <Form onSubmit={handleSubmit} className=\"w-100\" id=\"loginForm\">\r\n        <Form.Group>\r\n          <Form.Label>מה השם שלך?</Form.Label>\r\n          <Form.Control type=\"text\" ref={usernameRef} defaultValue={username} required /><br />\r\n          <Form.Control type=\"text\" ref={uuidRef} placeholder=\"קיבלת קוד? הכנס אותו לכאן\" />\r\n        </Form.Group>\r\n\r\n        <div>\r\n          <a href=\"#\" onClick={copyUuid}>{newUuid}</a>\r\n          &nbsp; | &nbsp;\r\n          <a\r\n            href={`whatsapp://send?text=${whatsappMessage}`}\r\n          >\r\n            שליחה בווטסאפ למדוייט/ת\r\n          </a>\r\n        </div>\r\n\r\n        <Button type=\"submit\" className=\"mt-2\" ref={loginForm}>להתחלת המשחק</Button>\r\n\r\n        <Instructions />\r\n\r\n      </Form>\r\n\r\n\r\n    </Container>\r\n  );\r\n\r\n}","C:\\xampp\\htdocs\\kahoodate\\kahoodate\\client\\src\\contexts\\AnswerProvider.js",[],"C:\\xampp\\htdocs\\kahoodate\\kahoodate\\client\\src\\components\\TheGame\\Dashboard\\Dashboard.js",["79","80","81"],"import React, { useState, useEffect, useCallback } from 'react';\nimport '../../../css/emoji.css'\nimport WordDisplay from '../WordDisplay/WordDisplay';\nimport Answer from '../Answer/Answer'\nimport NextQuest from '../NextQuest/NextQuest'\nimport Waiting from '../Waiting/Waiting'\nimport { useAnswer } from '../../../contexts/AnswerProvider';\nimport { useQuestion } from '../../../contexts/QuestionProvider';\n\n\nexport default function Dashboard() {\n    const [alreadySent, setAlreadySent] = useState(false)\n    const { answerMode } = useAnswer()\n    const { question, my_answer, setCallbackFunc } = useQuestion()\n    \n    // that's how i send to the parent component (QuestionProvider) the setAlreadySent state-setter\n    useEffect( () => {\n            setCallbackFunc(()=