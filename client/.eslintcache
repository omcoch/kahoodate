t value = {\r\n        question: question,\r\n        my_answer: my_answer,\r\n        setMyAnswer: setMyAnswer,\r\n        requestQuestion: requestQuestion,\r\n        setCallbackFunc: setCallbackFunc\r\n    }\r\n\r\n    return (\r\n        <QuestionContext.Provider value={value}>\r\n            {children}\r\n        </QuestionContext.Provider>\r\n    )\r\n}","C:\\xampp\\htdocs\\kahoodate\\kahoodate\\client\\src\\components\\Instructions\\Instructions.js",[],"C:\\xampp\\htdocs\\kahoodate\\kahoodate\\client\\src\\utils\\globals.js",[],"C:\\xampp\\htdocs\\kahoodate\\kahoodate\\client\\src\\components\\TheGame\\NewGameWaiting\\NewGameWaiting.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":38,"column":8,"nodeType":"88","endLine":38,"endColumn":14,"suggestions":"89"},{"ruleId":"86","severity":1,"message":"90","line":50,"column":8,"nodeType":"88","endLine":50,"endColumn":16,"suggestions":"91"},{"ruleId":"86","severity":1,"message":"92","line":23,"column":8,"nodeType":"88","endLine":23,"endColumn":10,"suggestions":"93"},{"ruleId":"94","severity":1,"message":"95","line":31,"column":25,"nodeType":"96","endLine":31,"endColumn":134},{"ruleId":"94","severity":1,"message":"95","line":16,"column":7,"nodeType":"96","endLine":16,"endColumn":33},{"ruleId":"86","severity":1,"message":"97","line":36,"column":8,"nodeType":"88","endLine":36,"endColumn":16,"suggestions":"98"},"no-native-reassign",["99"],"no-negated-in-lhs",["100"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'Mode', 'setMode', and 'username'. Either include them or remove the dependency array. If 'setMode' changes too often, find the parent component that defines it and wrap that definition in useCallback.","ArrayExpression",["101"],"React Hook useEffect has a missing dependency: 'exitGame'. Either include it or remove the dependency array.",["102"],"React Hook useEffect has a missing dependency: 'setCallbackFun